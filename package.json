{
  "name": "ipfs-desktop",
  "version": "1.0.1",
  "description": "ipfs desktop",
  "main": "index.js",
  "scripts": {
    "dev": "electron . dev",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --platform=all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zy445566/ipfs-desktop.git"
  },
  "keywords": [
    "ipfs",
    "desktop",
    "electron"
  ],
  "author": "zy445566",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zy445566/ipfs-desktop/issues"
  },
  "homepage": "https://github.com/zy445566/ipfs-desktop#readme",
  "devDependencies": {
    "electron": "^3.0.8",
    "electron-builder": "^20.34.0"
  },
  "build": {
    "appId": "io.ipfs.desktop",
    "mac": {
      "target": ["dmg"],
      "asarUnpack":["ipfs"]
    },
    "win": {
      "target": ["nsis"],
      "asarUnpack":["ipfs"]
    },
    "linux": {
      "target": ["AppImage"],
      "asarUnpack":["ipfs"]
    }
  }
}
